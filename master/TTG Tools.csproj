<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <SupportedOSPlatformVersion>6.1</SupportedOSPlatformVersion>
    <NoWarn>$(NoWarn);CA1416</NoWarn>
    <RootNamespace>TTG_Tools</RootNamespace>
    <AssemblyName>TTG Tools</AssemblyName>
    <ApplicationIcon>ttgtool.ico</ApplicationIcon>
    <Platforms>AnyCPU</Platforms>
    <PlatformTarget>x64</PlatformTarget>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\IconSave.ico" />
    <Content Include="Resources\IconSave.ico" />
    <Content Include="ttgtool.ico" />
  </ItemGroup>
</Project>
