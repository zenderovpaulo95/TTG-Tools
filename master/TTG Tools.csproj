<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <EnableWindowsTargeting>true</EnableWindowsTargeting>
    <RootNamespace>TTG_Tools</RootNamespace>
    <AssemblyName>TTG Tools</AssemblyName>
    <ApplicationIcon>ttgtool.ico</ApplicationIcon>
    <Platforms>AnyCPU</Platforms>
    <PlatformTarget>x64</PlatformTarget>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\IconSave.ico" />
    <Content Include="Resources\IconSave.ico" />
    <Content Include="ttgtool.ico" />
  </ItemGroup>
</Project>
